{
  "name": "demo-code-lint",
  "version": "1.0.0",
  "description": "代码检测demo，用于前端代码规范化",
  "main": "index.js",
  "dependencies": {},
  "devDependencies": {
    "husky": "^1.3.1",
    "lint-staged": "^8.0.4",
    "standard": "^12.0.1"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,md,css,html}": [
      "standard --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GarvenZhang/demo-code-lint.git"
  },
